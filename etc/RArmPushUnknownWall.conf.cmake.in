{
  "contact_targets":
  {
    // x/y relative to Left surface, z penetration depth (< 0)
    // x > 0 : higher, y > 0: farther from robot
    "left": [0, 0, -0.01],
    // x/y relative to Right surface, z penetration depth (< 0)
    // x > 0 : higher, y > 0: closer to robot
    "right": [0, 0, -0.01]
  },
  "contact_detection":
  {
    "UseForceSensor": true, // if true, use the force sensors to detect contact, otherwise use geometric sensor
    "ForceThreshold": 50.0, // Force threshold that triggers transition. Only used if UseForceSensor is true
    "ContactThreshold": 1e-5, // Contact distance threshold that triggers transition.
  },
  // Lifting height (m)
  "lift_height": 0.20,
  "push_depth": 0.10,
  // Trajectory task settings
  "trajectory":
  {
    "duration": 6.0,
    "stiffness": 500.0,
    "weight": 500
  },
  // CoM task settings
  "com":
  {
    "type": "com",
    "robotIndex": 0,
    "stiffness": 5.0,
    "weight": 5000
  },
  // FSM settings
  "Managed": false,
  "StepByStep": false,
  "IdleKeepState": true,
  "StatesLibraries": [
    "/usr/local/lib/mc_controller/fsm/states",
    "/usr/local/lib/mc_controller/r_arm_push_unknown_wall/states"
  ],
  "StatesFiles": [
    "/usr/local/lib/mc_controller/fsm/states/data",
    "/usr/local/lib/mc_controller/r_arm_push_unknown_wall/states/data"
  ],
  "robots": {
    "box": {
      "module": "object",
      "params": [
        "${MC_INT_OBJ_DESCRIPTION_PATH}",
        "big_box"
      ]
    },
    "ground":
    {
      "module": "env",
      "params": ["${MC_ENV_DESCRIPTION_PATH}", "ground"]
    }
  },
  "constraints": [
    {
      "type": "contact"
    },
  ],
  "collisions": [
    {
      "type": "collision",
      "r1Index": 0,
      "r2Index": 0,
      "useMinimal": true
    }
  ],
  "contacts": [
    {
      "r1": "hrp4",
      "r2": "ground",
      "r1Surface": "LeftFoot",
      "r2Surface": "AllGround"
    },
    {
      "r1": "hrp4",
      "r2": "ground",
      "r1Surface": "RightFoot",
      "r2Surface": "AllGround"
    },
    {
      "r1": "box",
      "r2": "ground",
      "r1Surface": "Bottom",
      "r2Surface": "AllGround"
    }
  ],
  "hrp4": {
    "posture": {
      "stiffness": 1.0,
      "weight": 10.0
    },
    "ff": {
      "stiffness": 2.0,
      "weight": 100.0
    },
    "leftEfTask": {
      "bodyName" : "l_wrist",
      "stiffness" : 20.0,
      "weight" : 1000.0,
      "bound" : 0.1
    },
    "rightEfTask": {
      "bodyName" : "r_wrist",
      "stiffness" : 5.0,
      "weight" : 200.0,
      "bound" : 0.1
    },
    "rightEfTaskDynamic":
    {
      "bodyName" : "r_wrist",
      "stiffness" : 5.0,
      "weight" : 200.0,
      "bound" : 0.1
    }
  },
  "qpEstimator":{
   "on": true,
   "solverName": "nlopt::LD_CCSAQ",
   "dim":3,
   "Qweight":1,
   "convergenceThreshold":1e-2,
   "useLagrangeMultiplier": true
  },
  "lcp":{
   "on": false,
   "solver": "nlopt::LD_CCSAQ",
   "solver2": "nlopt::LD_MMA",
   "solver3": "nlopt::LD_SLSQP",
   "dim":3,
   "convergenceThreshold":1e-2
  },
  "impact": {
    "constraints": {
      "jointVelocity": true,
      "jointTorque": {"on": true, "multiplier": 1.0 },
      "CoPLeft": false,
      "CoPRight": false,
      "SlippageLeft": false,
      "SlippageRight": false,
      "frictionWithImpulse": true,
      "copWithImpulse": {"on": true, "contactArea": [-0.09, 0.09, -0.05, 0.05]},
      "zmpWithImpulse": {"on": true, "supportPolygon": [-0.08, 0.08, -0.12, 0.12], "debug": true}
    },
    "estimation": {
      "coeFrictionDeduction": 0.2,
      "coeRestitution": 0.3,
      "delta_dt-old": 0.005,
      "delta_dt": 0.005,
      "useLinearJacobian": true,
      "contactBodies": ["l_sole", "r_sole"],
      "impactBodies": ["r_wrist"],
      "impactBodies-old": ["r_wrist", "l_wrist"],
      "impactBodyName": "r_wrist",
      "end-effectors-back": [ "l_sole", "r_sole", "l_wrist", "r_wrist"],
      "end-effectors": [ "l_sole", "r_sole", "r_wrist"]
    }
  },
  "states": {
    "Prepare":
    {
      "base": "PrepareHand",
      "raiseHandOffset" : [0.15, -0.05, 0.2],
      "EfThreshold" : 0.05
    },
    "Contact":
    {
      "base": "DynamicContact", // C++ state
      "contactVelocity" : [0.4, 0.0, 0.0], //
      "pushDepth":  [1.0, 0.0, 0.0], // Target position that should generate the impact
      // Velocity control for the end-effector
      "rightEfStiffness": 0.0,
      "rightEfDamping": 300.0
    },
    "Stabilize":
    {
      "base": "StabilizeContact", // C++ state
      "contactVelocity" : [0.3, 0.0, 0.0], //
      "surfaceName": "rightpalm?",
      "targetForce": [20.0, 0.0, 0.0],
      // Velocity control for the end-effector
      "rightEfStiffness": 0.0,
      "rightEfDamping": 300.0
    }
  },
  "init": "Prepare",
  "transitions": [
    [ "Prepare", "RightHandReady", "Contact", "Auto" ]
    // [ "Contact", "ok", "Stabilize", "Auto" ]
  ]
}
